{
    "name": "ml.marchionni.cuaderno",
    "displayName": "HelloCordova",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "start": "cordova run browser",
        "dev": "live-server www",
        "docs": "jsdoc www/js -c jsdoc.json -d docs"
    },
    "keywords": [
        "ecosystem:cordova"
    ],
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "devDependencies": {
        "better-docs": "^2.3.2",
        "cordova-android": "^9.1.0",
        "cordova-browser": "^6.0.0",
        "cordova-plugin-whitelist": "^1.3.4",
        "jsdoc": "^3.6.7"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "browser",
            "android"
        ]
    },
    "dependencies": {
        "live-server": "^1.2.1"
    }
}
